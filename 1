  var second_clicked_card=false;
  var first_clicked_card_src=null;
  var first_back_card_id ;
  var first_front_card_id ;
 function card_click(card_back_id,card_front_id)
 {  console.log("in card_click()", card_back_id);
    //$('#card_back_id').hide();
   //$('#card_back1').css('display','none');
    $(card_back_id).addClass('selected');

     console.log(card_front_id);
       
          
          
    var card_front_src = $(card_front_id).attr('src');
     $(card_back_id).hide();
    if(!second_clicked_card)
    {
         console.log('it is the first clicked card');
          first_back_card_id = card_back_id;
          first_front_card_id = card_front_id;
         console.log("card_front_src : ",card_front_src);
         first_clicked_card_src = card_front_src;
         second_clicked_card = true;
   }

    else 
    {   
      var card_front_src = $(card_front_id).attr('src');
           
      if (first_clicked_card_src==card_front_src) 
      {
         console.log("we are the same!",$(card_front_id));
    
       $(card_back_id).show().removeClass("back-face").addClass("hi");
          //$(first_clicked_card_id).hide();
        second_clicked_card=false;
        first_clicked_card_src=null;
      }
      else 
      {
         console.log("Try again!");

         $(card_back_id).show(550);

          $(first_back_card_id).show(250);

           /*div.appendTo(card_back_id);*/
        second_clicked_card=false;
        first_clicked_card_src=null;
      } ;

   }

}


